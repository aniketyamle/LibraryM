<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXButton?>
<DialogPane fx:id="material" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Package.Return">
    <content>
        <GridPane alignment="top_left" hgap="20" prefHeight="400.0" prefWidth="600.0"  stylesheets="@styles.css" style="-fx-font: 14px Helvetica" vgap="5">
            <padding>
                <Insets left="100" top="100" />
            </padding>
            <JFXTextField fx:id="Book" promptText="Book Name" GridPane.rowIndex="1" />


            <JFXTextField fx:id="FirstName" promptText="First Name" GridPane.columnIndex="0" GridPane.rowIndex="5" />
            <JFXTextField fx:id="LastName" promptText="Last Name" GridPane.columnIndex="1" GridPane.rowIndex="5" />

            <JFXButton fx:id="bluebutton" onAction="#Returnit" text="Return" GridPane.columnIndex="0" GridPane.rowIndex="7" />

            <columnConstraints>
                <ColumnConstraints />
                <ColumnConstraints />
                <ColumnConstraints />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            </rowConstraints>
        </GridPane>

    </content>
</DialogPane>
