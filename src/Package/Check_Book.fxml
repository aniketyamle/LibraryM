<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<?import com.jfoenix.controls.JFXButton?>
<AnchorPane stylesheets="@styles.css" fx:id="pane" prefHeight="624.0" prefWidth="887.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Package.Check_book">
    <padding>
        <Insets left="100" top="100" />
    </padding>
    <children>
      <TableView fx:id="tableView" layoutX="49.0" layoutY="22.0" prefHeight="467.0" prefWidth="830.0">
          <columns>
              <TableColumn fx:id="isbn" prefWidth="75.0" text="ISBN" />
              <TableColumn fx:id="name" prefWidth="207.0" text="Name" />
              <TableColumn fx:id="author" prefWidth="151.0" text="Author" />
              <TableColumn fx:id="subject" prefWidth="167.0" text="Subject" />
              <TableColumn fx:id="status" prefWidth="227.0" text="Status" />
          </columns>
      </TableView>
      <JFXButton fx:id="normalbutton" layoutX="790.0" layoutY="553.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="89.0" text="Back" />
      <Pane layoutX="49.0" layoutY="517.0" prefHeight="72.0" prefWidth="387.0">
         <children>
            <Label layoutX="2.0" layoutY="14.0" prefHeight="17.0" prefWidth="98.0" text="Add a new Book" />
            <JFXButton fx:id="jfxloginbutton" layoutX="2.0" layoutY="39.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="89.0" text="Add" onAction="#addBooks" />
         </children>
      </Pane>
   </children>
</AnchorPane>


