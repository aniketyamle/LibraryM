<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import com.jfoenix.controls.JFXButton?>
<AnchorPane  stylesheets="@styles.css" fx:id="pane" prefHeight="624.0" prefWidth="887.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Package.Check_transaction">
    <padding>
        <Insets left="100" top="100" />
    </padding>
    <children>
        <TableView fx:id="tableView" layoutX="49.0" layoutY="22.0" prefHeight="467.0" prefWidth="830.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="69.0" text="Id" />
                <TableColumn fx:id="firstname" prefWidth="130.0" text="First Name" />
                <TableColumn fx:id="lastname" prefWidth="135.0" text="Last Name" />
                <TableColumn fx:id="book" prefWidth="180.0" text="Book" />
                <TableColumn fx:id="issuedate" minWidth="5.0" prefWidth="103.0" text="Issue Date" />
                <TableColumn fx:id="returndate" maxWidth="200.0" prefWidth="121.0" text="Return Date" />
                <TableColumn fx:id="fine" prefWidth="91.0" text="Fine" />
            </columns>
        </TableView>
        <JFXButton fx:id="normalbutton" layoutX="838.0" layoutY="568.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <JFXButton fx:id="jfxloginbutton" layoutX="49.0" layoutY="568.0" mnemonicParsing="false" text="Show Incomplete Only" onAction="#showDialog"/>
    </children>
</AnchorPane>
