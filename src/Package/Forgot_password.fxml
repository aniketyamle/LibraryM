<DialogPane fx:id="material" prefHeight="363.0" prefWidth="606.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Package.Forgot_Password">
    <content>
        <Pane>
         <children>
            <JFXTextField promptText="Enter your name " fx:id="username" layoutX="46.0" layoutY="96.0" />
            <Label layoutX="46.0" layoutY="138.0" text="Answer yor security question" />
             <JFXComboBox fx:id="combox" layoutX="46.0" layoutY="159.0" prefHeight="25.0" prefWidth="201.0">
                 <items>
                     <FXCollections fx:factory="observableArrayList">
                         <String fx:value="Your First Book" />
                         <String fx:value="Your Favourite Movie" />
                     </FXCollections>
                 </items>
             </JFXComboBox>
             <JFXPasswordField  fx:id="answer" layoutX="46.0" layoutY="200.0" prefHeight="25.0" prefWidth="204.0" />
            <Label fx:id="label" layoutX="46.0" layoutY="282.0" prefHeight="17.0" prefWidth="246.0" />
            <JFXButton fx:id="normalbutton" onAction="#check" layoutX="46.0" layoutY="249.0" mnemonicParsing="false" text="Check" />

         </children></Pane>
    </content>
